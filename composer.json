{
  "name": "robotdance/php-i18n",
  "type": "library",
  "description": "Simple I18N library for PHP",
  "keywords": ["php", "i18n", "library"],
  "license": "MIT",
  "authors": [{
    "name": "Marcos Menegazzo",
    "email": "marcos.menegazzo@gmail.com"
  }],
  "require": {
    "php": ">=5.3.2",
    "robotdance/php-app-config": "0.0.8",
    "robotdance/php-arguments": "0.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.24",
    "phpdocumentor/phpdocumentor": "2.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "robotdance\\": "src",
      "robotdance\\tests\\": "tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "robotdance\\tests\\": "tests"
    }
  }
}
